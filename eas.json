{
  "cli": { "version": ">= 13.0.0", "appVersionSource": "local" },
  "build": {
    "simulator": {
      "ios": { "simulator": true }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": { "resourceClass": "m-medium" },
      "android": { "gradleCommand": ":app:assembleDebug" }
    },
    "production": {
      "autoIncrement": "version",
      "ios": { "resourceClass": "m-medium" }
    }
  },
  "submit": { "production": {} }
}
